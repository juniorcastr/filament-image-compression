{
    "name": "condosmart/filament-image-compression",
    "description": "Plugin Filament para compressão automática de imagens com conversão para WebP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CondoSmart",
            "email": "dev@condosmart.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "intervention/image": "^3.0",
        "spatie/laravel-package-tools": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CondoSmart\\FilamentImageCompression\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CondoSmart\\FilamentImageCompression\\FilamentImageCompressionServiceProvider"
            ]
        }
    }
},
    "minimum-stability": "dev",
    "prefer-stable": true
}